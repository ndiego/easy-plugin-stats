{
  "name": "easy-plugin-stats",
  "version": "2.0.0",
  "description": "Easily display stats associated with plugins hosted on WordPress.org.",
  "author": "Nick Diego",
  "main": "build/index.js",
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndiego/easy-plugin-stats.git"
  },
  "scripts": {
    "build": "wp-scripts build",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:js:src": "wp-scripts lint-js ./src",
    "lint:js:src:fix": "wp-scripts lint-js ./src --fix",
    "start": "wp-scripts start",
    "packages-update": "wp-scripts packages-update",
    "update-pot": "wp i18n make-pot . languages/easy-plugin-stats.pot --exclude=src",
    "plugin-zip": "wp-scripts plugin-zip"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@wordpress/icons": "^9.48.0",
